{
  "name": "authentication-ts",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx serve authentication-ts"],
        "parallel": false
      },
      "configurations": {
        "watch": {
          "watch": true
        }
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/",
        "main": "./src/index.ts",
        "tsConfig": "./tsconfig.json"
      }
    }
  }
}
