{
    "name": "authentication-api",
    "targets": {
        "serve": {
            "executor": "@nx/ts:node",
            "options": {
                "main": "./src/index.ts",
                "tsConfig": "./tsconfig.json",
                "watch": true
            }
        },
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/authentication-api",
                "main": "./src/index.ts",
                "tsConfig": "./tsconfig.json"
            }
        }
    }
}