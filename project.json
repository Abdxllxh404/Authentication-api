{
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/apps/authentication-api",
                "main": "apps/authentication-api/src/main.ts",
                "tsConfig": "apps/authentication-api/tsconfig.app.json"
            }
        },
        "serve": {
            "executor": "@nx/node:execute",
            "options": {
                "buildTarget": "authentication-api:build",
                "runtime": "node",
                "main": "apps/authentication-api/src/main.ts"
            }
        }
    }
}